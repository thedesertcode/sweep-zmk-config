#include "cradio.dtsi"

&kscan0 {
    debounce-press-ms = <0>;
    debounce-release-ms = <5>;
    debounce-scan-period-ms = <1>;
    wakeup-source;
};

/ {
    battery_peripheral_0: battery_peripheral_0 {
        compatible = "zmk,battery-ble-peripheral";
        status = "okay";
    };
};
